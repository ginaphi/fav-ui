{
	"name": "fav-ui",
	"private": true,
	"scripts": {
		"build": "turbo build",
		"dev": "turbo dev",
		"start": "turbo start",
		"lint": "turbo lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"clean": "rm -rf .turbo node_modules .next package-lock.json",
		"clean:workspaces": "turbo clean"
	},
	"devDependencies": {
		"prettier": "^3.2.5",
		"turbo": "^2.1.2",
		"typescript": "^5.4.5"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "npm@9.4.2",
	"workspaces": [
		"apps/*",
		"packages/*"
	]
}
